{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "serviceworker": true
  },
  "globals": {
    "APP": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
      "allowImportExportEverywhere": true
    }
  }
}